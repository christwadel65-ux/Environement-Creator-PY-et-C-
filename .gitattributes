# Git LFS tracking for published binary artifacts only
bin/Release/**/publish/*.exe filter=lfs diff=lfs merge=lfs -text
bin/Release/**/publish/*.dll filter=lfs diff=lfs merge=lfs -text
bin/Release/**/publish/*.pdb filter=lfs diff=lfs merge=lfs -text
bin/Release/**/publish/*.baml filter=lfs diff=lfs merge=lfs -text
# Optional: archives/packages in publish
bin/Release/**/publish/*.zip filter=lfs diff=lfs merge=lfs -text
bin/Release/**/publish/*.7z filter=lfs diff=lfs merge=lfs -text
bin/Release/**/publish/*.nupkg filter=lfs diff=lfs merge=lfs -text

# Do NOT track source files with LFS
*.cs linguist-language=C#
*.xaml linguist-language=XAML
*.md linguist-language=Markdown
