# Git LFS tracking for root publish binary artifacts only
/publish/*.exe filter=lfs diff=lfs merge=lfs -text
/publish/*.dll filter=lfs diff=lfs merge=lfs -text
/publish/*.pdb filter=lfs diff=lfs merge=lfs -text
/publish/*.baml filter=lfs diff=lfs merge=lfs -text
# Optional: archives/packages in publish
/publish/*.zip filter=lfs diff=lfs merge=lfs -text
/publish/*.7z filter=lfs diff=lfs merge=lfs -text
/publish/*.nupkg filter=lfs diff=lfs merge=lfs -text

# Do NOT track source files with LFS
*.cs linguist-language=C#
*.xaml linguist-language=XAML
*.md linguist-language=Markdown
