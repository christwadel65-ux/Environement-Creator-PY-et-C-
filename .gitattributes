# Git LFS tracking for binary artifacts
*.exe filter=lfs diff=lfs merge=lfs -text
*.dll filter=lfs diff=lfs merge=lfs -text
*.pdb filter=lfs diff=lfs merge=lfs -text
*.baml filter=lfs diff=lfs merge=lfs -text
# Optional: archives and packages
*.zip filter=lfs diff=lfs merge=lfs -text
*.7z filter=lfs diff=lfs merge=lfs -text
*.nupkg filter=lfs diff=lfs merge=lfs -text

# Do NOT track source files with LFS
*.cs linguist-language=C#
*.xaml linguist-language=XAML
*.md linguist-language=Markdown
